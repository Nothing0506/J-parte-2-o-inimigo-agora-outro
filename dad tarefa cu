#include<iostream>
#include<math.h>


using namespace std;

int main(){
    int n, val;
        cout<"Quantos dados vao ser inseridos?"<<endl;
            cin>>n;
            int dados[n];
        cout<<"Insira os dados"<<endl;
        for(int i=0; i<n; i++){
            cin>>val;
            dados[i]=val;
        }
        

}

